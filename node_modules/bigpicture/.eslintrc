{
  "extends": "eslint:recommended",
  "env": {
    "browser": true
  },
  "rules": {
    "no-empty": 1,
    "no-console": 1,
    "no-empty-pattern": 0,
    "no-cond-assign": 1,
    "semi": ["warn", "never"],
    "camelcase": 1,
    "comma-style": 2,
    "comma-dangle": 1,
    "indent": [2, "tab", {
      "SwitchCase": 1
    }],
    "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
    "no-trailing-spaces": [2, {
      "skipBlankLines": true
    }],
    "max-nested-callbacks": [2, 3],
    "no-eval": 2,
    "no-implied-eval": 2,
    "no-new-func": 2,
    "guard-for-in": 2,
    "eqeqeq": 1,
    "no-else-return": 2,
    "no-redeclare": 2,
    "no-dupe-keys": 2,
    "radix": 1,
    "strict": [2, "never"],
    "no-shadow": 0,
    "no-delete-var": 2,
    "no-undef-init": 2,
    "no-shadow-restricted-names": 2,
    "handle-callback-err": 0,
    "no-lonely-if": 2,
    "keyword-spacing": 2,
    "no-useless-concat": 2
  }
}